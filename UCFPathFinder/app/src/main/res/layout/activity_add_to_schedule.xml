<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".AddToSchedule"
    tools:layout_editor_absoluteY="81dp">

    <Spinner
        android:id="@+id/spinner_building_addToSchedule"
        android:layout_width="137dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="17dp"
        android:layout_marginEnd="16dp"
        android:layout_marginRight="16dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/textView_buildingID_addToSchedule"
        app:layout_constraintTop_toBottomOf="@+id/spinner_day_addToSchedule" />

    <TextView
        android:id="@+id/textView_buildingID_addToSchedule"
        android:layout_width="125dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="32dp"
        android:layout_marginLeft="32dp"
        android:layout_marginTop="16dp"
        android:text="Building"
        android:textSize="18sp"
        app:layout_constraintEnd_toStartOf="@+id/spinner_building_addToSchedule"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView_day_addToSchedule" />

    <Spinner
        android:id="@+id/spinner_day_addToSchedule"
        android:layout_width="137dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="17dp"
        android:layout_marginEnd="16dp"
        android:layout_marginRight="16dp"
        android:entries="@array/academic_days"
        app:layout_constraintBottom_toTopOf="@+id/spinner_building_addToSchedule"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/textView_day_addToSchedule"
        app:layout_constraintTop_toBottomOf="@+id/spinner_endTime_addToSchedule" />

    <TextView
        android:id="@+id/textView_day_addToSchedule"
        android:layout_width="125dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="32dp"
        android:layout_marginLeft="32dp"
        android:layout_marginTop="16dp"
        android:text="Day"
        android:textSize="18sp"
        app:layout_constraintBottom_toTopOf="@+id/textView_buildingID_addToSchedule"
        app:layout_constraintEnd_toStartOf="@+id/spinner_day_addToSchedule"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView_endTime_addToSchedule" />

    <TextView
        android:id="@+id/textView_endTime_addToSchedule"
        android:layout_width="125dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="32dp"
        android:layout_marginLeft="32dp"
        android:layout_marginTop="16dp"
        android:text="End"
        android:textSize="18sp"
        app:layout_constraintBottom_toTopOf="@+id/textView_day_addToSchedule"
        app:layout_constraintEnd_toStartOf="@+id/spinner_endTime_addToSchedule"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView_startTime_addToSchedule" />

    <TextView
        android:id="@+id/textView_startTime_addToSchedule"
        android:layout_width="125dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="32dp"
        android:layout_marginLeft="32dp"
        android:layout_marginTop="16dp"
        android:text="Start"
        android:textSize="18sp"
        app:layout_constraintBottom_toTopOf="@+id/textView_endTime_addToSchedule"
        app:layout_constraintEnd_toStartOf="@+id/spinner_startTime_addToSchedule"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView_term_addToSchedule" />

    <Spinner
        android:id="@+id/spinner_endTime_addToSchedule"
        android:layout_width="137dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="17dp"
        android:layout_marginEnd="16dp"
        android:layout_marginRight="16dp"
        android:entries="@array/academic_times"
        app:layout_constraintBottom_toTopOf="@+id/spinner_day_addToSchedule"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/textView_endTime_addToSchedule"
        app:layout_constraintTop_toBottomOf="@+id/spinner_startTime_addToSchedule" />

    <Spinner
        android:id="@+id/spinner_startTime_addToSchedule"
        android:layout_width="137dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        android:layout_marginEnd="16dp"
        android:layout_marginRight="16dp"
        android:entries="@array/academic_times"
        app:layout_constraintBottom_toTopOf="@+id/spinner_endTime_addToSchedule"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/textView_startTime_addToSchedule"
        app:layout_constraintTop_toBottomOf="@+id/spinner_term_addToSchedule" />

    <Spinner
        android:id="@+id/spinner_term_addToSchedule"
        android:layout_width="137dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="17dp"
        android:layout_marginEnd="16dp"
        android:layout_marginRight="16dp"
        android:entries="@array/academic_terms"
        app:layout_constraintBottom_toTopOf="@+id/spinner_startTime_addToSchedule"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/textView_term_addToSchedule"
        app:layout_constraintTop_toBottomOf="@+id/spinner_year_addToSchedule" />

    <TextView
        android:id="@+id/textView_term_addToSchedule"
        android:layout_width="125dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="32dp"
        android:layout_marginLeft="32dp"
        android:layout_marginTop="16dp"
        android:text="Term"
        android:textSize="18sp"
        app:layout_constraintBottom_toTopOf="@+id/textView_startTime_addToSchedule"
        app:layout_constraintEnd_toStartOf="@+id/spinner_term_addToSchedule"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView_year_addToSchedule" />

    <EditText
        android:id="@+id/editText_className_addToSchedule"
        android:layout_width="361dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:layout_marginLeft="8dp"
        android:layout_marginTop="32dp"
        android:layout_marginEnd="8dp"
        android:layout_marginRight="8dp"
        android:layout_marginBottom="8dp"
        android:autofillHints=""
        android:ems="10"
        android:hint="@string/course_name"
        android:inputType="textPersonName"
        app:layout_constraintBottom_toTopOf="@+id/editText_classID_addToSchedule"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <EditText
        android:id="@+id/editText_classID_addToSchedule"
        android:layout_width="361dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:layout_marginLeft="8dp"
        android:layout_marginTop="8dp"
        android:layout_marginEnd="8dp"
        android:layout_marginRight="8dp"
        android:ems="10"
        android:hint="Course ID"
        android:inputType="textPersonName"
        app:layout_constraintBottom_toTopOf="@+id/spinner_year_addToSchedule"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/editText_className_addToSchedule" />

    <Spinner
        android:id="@+id/spinner_year_addToSchedule"
        android:layout_width="137dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="17dp"
        android:layout_marginEnd="16dp"
        android:layout_marginRight="16dp"
        android:entries="@array/future_years"
        app:layout_constraintBottom_toTopOf="@+id/spinner_term_addToSchedule"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/textView_year_addToSchedule"
        app:layout_constraintTop_toBottomOf="@+id/editText_classID_addToSchedule" />

    <TextView
        android:id="@+id/textView_year_addToSchedule"
        android:layout_width="125dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="32dp"
        android:layout_marginLeft="32dp"
        android:layout_marginTop="16dp"
        android:text="Year"
        android:textSize="18sp"
        app:layout_constraintBottom_toTopOf="@+id/textView_term_addToSchedule"
        app:layout_constraintEnd_toStartOf="@+id/spinner_year_addToSchedule"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/editText_classID_addToSchedule" />

    <Button
        android:id="@+id/button_cancel_addToSchedule"
        android:layout_width="320dp"
        android:layout_height="0dp"
        android:layout_marginStart="32dp"
        android:layout_marginLeft="32dp"
        android:layout_marginEnd="32dp"
        android:layout_marginRight="32dp"
        android:layout_marginBottom="8dp"
        android:text="CANCEL"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <Button
        android:id="@+id/button_add_addToSchedule"
        android:layout_width="320dp"
        android:layout_height="0dp"
        android:layout_marginStart="32dp"
        android:layout_marginLeft="32dp"
        android:layout_marginEnd="32dp"
        android:layout_marginRight="32dp"
        android:text="ADD"
        app:layout_constraintBottom_toTopOf="@+id/button_cancel_addToSchedule"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

</android.support.constraint.ConstraintLayout>